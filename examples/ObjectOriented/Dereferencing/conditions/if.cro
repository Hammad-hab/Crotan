# block "Application"
    
define(Master "Hammad")
if (is_equal(Master "Hammad")) as condition

|condition then| as then

then(fn callback {
    println("Hammad is the creator of the language!")
}) as else_container
    
|else_container else| as else

else(declare callback {
    println("Whoever it is, he is not the creator of this language")
})

# end "Application"
